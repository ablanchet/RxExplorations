<Window x:Class="LogExample.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Command="{Binding OpenLogWindow}">Open log window</Button>
            <Button Command="{Binding OpenSubWindow}">Open sub window</Button>
            <Button Command="{Binding TriggerGC}">Trigger GC</Button>
            <Button Command="{Binding CreateLog}">Create log</Button>
        </StackPanel>
    </Grid>
</Window>
